@using Ifa.Helpers
@using Ifa.Model
@model IEnumerable<Tag>

    
        <ul class="pagination justify-content-center danger" >
            @foreach (var tag in Model
                .RemoveTagsByType(typeof(FirstPage),
                typeof(LastPage),
                typeof(Gap)))
            {
                @Html.DisplayFor(m=>tag)
            }
        </ul>

   


